<% if session[:user_role] == 'Admin' %>
    <h3>Welcome admin, <%= current_admin.full_name -%></h3>
    <%= link_to 'Event Types', event_types_path %><br>
    <%= link_to 'Events', events_path %><br>
    <%= link_to 'Roles', roles_path %><br>
    <%= link_to 'RSVP', rsvps_path %><br>
    <%= link_to 'Storage Keys', storage_keys_path %><br>
    <%= link_to 'Users', users_path %><br>
    <%= link_to 'Instruments', instruments_path %><br>
    <%= link_to 'Help', help_menu_index_path %><br>

    <%= link_to "Sign Out", destroy_admin_session_path %>
<% elsif session[:user_role] == 'Member' %>
    <h3>Welcome member, <%= current_admin.full_name -%></h3>
    <%= link_to "calendar", events_path %><br>
    <%= link_to "help", help_menu_index_path %>

    <%= link_to "Sign Out", destroy_admin_session_path %>
<% else %>
    <h3>You are not a member, <%= current_admin.full_name -%></h3>

    <%= link_to "Back", destroy_admin_session_path %>
<% end %>
